Question:-
https://www.pepcoding.com/resources/online-java-foundation/2d-arrays/2d-arrays-demo-official/ojquestion

Code:-

#include<iostream>
using namespace std;
int main(){
	int n,m; // n = no_of_rows , m = no_of_columns
	cin>>n>>m;
	long int a[n][m]; // 2d array
	for(int i=0;i<n;i++){
		for(int j=0;j<m;j++){
			cin>>a[i][j];
		}
    }
    for(int i=0;i<n;i++){
		for(int j=0;j<m;j++){
			cout<<a[i][j]<<" ";
		}
		cout<<"\n";
    }
	return 0;
}
