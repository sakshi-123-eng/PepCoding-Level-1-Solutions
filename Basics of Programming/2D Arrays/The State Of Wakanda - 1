Question:-
https://www.pepcoding.com/resources/online-java-foundation/2d-arrays/wakanda-1-official/ojquestion

Code:-

#include<bits/stdc++.h>
using namespace std;
int main(){
    int m,n;
    cin>>n>>m;
    int arr[n][m];
    for(int i=0;i<n;i++){
        for(int j=0;j<m;j++){
            cin>>arr[i][j];
        }
        
    }int flag=0;
 for(int i=0;i<m;i++){
        if(flag==0){
        for(int j=0;j<n;j++){
           
            cout<<arr[j][i]<<endl;
            flag=1;   
        }    
     }
        
        else{
        for(int j=n-1;j>=0;j--){
            cout<<arr[j][i]<<endl; 
              flag=0;
        }
        
    }
 }
    
}
