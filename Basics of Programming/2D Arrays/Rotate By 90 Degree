Question:-
https://www.pepcoding.com/resources/online-java-foundation/2d-arrays/rotate-by-90-degree-official/ojquestion

Code:-

#include<iostream>
using namespace std;
int main(){
	int n;
	cin>>n;
	long int a[n][n];
	for(int i=0;i<n;i++){
		for(int j=0;j<n;j++){
			cin>>a[i][j];
		}
	}

for(int j=0;j<n;j++){
		for(int i=n-1;i>=0;i--){
			cout<<a[i][j]<<" ";
		}
		cout<<"\n";
	}
	
	return 0;
}
