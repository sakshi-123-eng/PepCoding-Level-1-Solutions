Question:-
https://www.pepcoding.com/resources/online-java-foundation/function-and-arrays/reverse-an-array-official/ojquestion

Code:-

#include<bits/stdc++.h>
using namespace std;
int main(){
	long int n;
	cin>>n;
	long int a[n],b[n];
	for(long int i=0;i<n;i++){
		cin>>a[i];
	}
	long int j=0;
	for(long int i=n-1;i>=0;i--){
		b[j]=a[i];
		j++;
	}
	for(long int i=0;i<n;i++){
		cout<<b[i]<<" ";
	}
	
	
	return 0;
}
