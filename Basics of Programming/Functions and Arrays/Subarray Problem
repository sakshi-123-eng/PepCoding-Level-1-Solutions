Question:-
https://www.pepcoding.com/resources/online-java-foundation/function-and-arrays/subarray-problem-official/ojquestion

Code:-

#include<bits/stdc++.h>
using namespace std;
int main(){
	int n;
	cin>>n;
	int a[n];
	for(int i=0;i<n;i++){
		cin>>a[i];
	}
	for(int i=0;i<n;i++){
		int s=i;
		while(s<n){
			for(int j=i;j<=s;j++){
				cout<<a[j]<<"	";
			}
			s++;
			cout<<"\n";
		}
	}
	return 0;
}
