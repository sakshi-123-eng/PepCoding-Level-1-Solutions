Code:-

#include<iostream>
#include<algorithm>
#include<cmath>
using namespace std;
int main(){
	long int n ,s=1,i=1;
	cin>>n;
	while(s<=n){
		if(i==1){
			cout<<"1"<<"\n";
			i++;
			continue;
		}
		if(i%2 !=0){
			s=s+i;
			if(s<=n){
				cout<<s<<"\n";
			}
			
		}
		i++;
	}
	return 0;
}
